<?xml version="1.0"?>
<PropertyList>

 <path>landinglight.ac</path>
 <nopreview/>


 
<effect>
   <inherits-from>Aircraft/737-800YV/Models/lights/ALS/landinglightR</inherits-from>
   <object-name>landinglight</object-name>
 </effect>
 

 
 <animation>
  <type>scale</type>
  <x-offset>5.15</x-offset>
  <y-offset>5.15</y-offset>
  <z-offset>5.15</z-offset>
 </animation>
 

 
<animation>
  <type>dist-scale</type>
  <object-name>landinglight</object-name>
  <interpolation>
   <entry>
    <ind>0</ind>
    <dep>0.5</dep>
   </entry>
   <entry>
    <ind>300</ind>
    <dep>2</dep>
   </entry>
   <entry>
    <ind>1500</ind>
    <dep>4</dep>
   </entry>
  </interpolation>
 </animation>
 
    <animation>
        <type>flash</type>
        <object-name>landinglight</object-name>
        <center>
            <x-m>0.0</x-m>
            <y-m>0.0</y-m>
            <z-m>0.0</z-m>
        </center>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
        <power>8</power>
        <factor>6</factor>
        <offset>0</offset>
        <two-sides type="bool">false</two-sides>
        <min>0.5</min>
        <max>1.5</max>
    </animation>

<light>
        <type>spot</type>
        <name>Spotlight</name>
        <position>
            <x-m>0</x-m>
            <y-m>0</y-m>
            <z-m>0</z-m>
        </position>
        <direction>
            <heading-deg>-3</heading-deg>
            <roll-deg>0</roll-deg>
            <pitch-deg>85</pitch-deg>
        </direction>
        <ambient>
            <r>1.0</r>
            <g>1.0</g>
            <b>1.0</b>
            <a>1.0</a>
        </ambient>
        <diffuse>
            <r>1.0</r>
            <g>1.0</g>
            <b>1.0</b>
            <a>0.7</a>
        </diffuse>
        <specular>
            <r>1.0</r>
            <g>1.0</g>
            <b>1.0</b>
            <a>0.7</a>
        </specular>
        <dim-factor>
            <property>systems/electrical/outputs/landing-lights-norm[2]</property>
            <factor>1</factor>
            <offset>0</offset>
            <min>0</min>
            <max>1</max>
        </dim-factor>
        <attenuation>
            <c>2</c>
            <l>0.01</l>
            <q>0.0001</q>
        </attenuation>
        <spot-exponent>100</spot-exponent>
        <spot-cutoff>20.0</spot-cutoff>
        <range-m>1000</range-m>
        <near-m>0.1</near-m>
        <far-m>1000</far-m>
    </light>

</PropertyList>
